proc tree(){
	if($1 < 5)	{
		Move($1);
		Turn(180);
		Move($1);
		Turn(180);
	} else {
		Move($1 * 0.3);
		Turn(330);
		tree($1* 0.66);
		Turn(30);
		Move($1 * 0.16);
		Turn(335);
		tree($1 * 0.5);
		Turn(25); 
		Move($1 * 0.3);
		Turn(335);
		tree($1 * 0.5);
		Turn(25);
		Move($1 * 0.16)
		Turn(180);
		Move($1);
		Turn(180);
	}


} 
   
tree(150);
